- form_for @nominee, :html => {:multipart => true} do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :content
    %br
    = f.text_area :content
  %p
    = f.label :tags
    %br
    = f.text_field :tag_list
  %p
    = f.file_field :image
  %p
    = f.label :rank
    %br
    = f.text_field :vote
  %p
    = f.submit "Submit"
